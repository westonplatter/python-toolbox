<h3>Packages</h3>

<table class="table">
  <thead>
    <tr>
      <th></th>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @packages.each do |package| %>
      <tr>
        <td><%= link_to "view", package_path(package), class: "btn btn-sm btn-default" %></td>
        <td><%= package.name %></td>
      </tr>
    <% end %>
  </tbody>

</table>
